# Increasing numbers pattern
for i in range(1, 6):
    for j in range(1, i+1):
        print(j, end="")
    print()

# Right-aligned number triangle
for i in range(1, 6):
    print(" " * (6-i-1), end="")
    for j in range(1, i+1):
        print(j, end="")
    print()

# Hollow square
n = 5
for i in range(1, n+1):
    for j in range(1, n+1):
        if i==1 or i==n or j==1 or j==n:
            print("*", end="")
        else:
            print(" ", end="")
    print()
