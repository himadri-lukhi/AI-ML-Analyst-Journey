age = int(input("Enter your age: "))

if 1 <= age <= 12:
    print("Child")
elif 13 <= age <= 17:
    print("Teen")
elif 18 <= age <= 59:
    print("Adult")
elif age >= 60:
    print("Senior")
else:
    print("Invalid age")

# Even/Odd
num = int(input("Enter a number: "))
if num % 2 == 0:
    print("Even")
else:
    print("Odd")
